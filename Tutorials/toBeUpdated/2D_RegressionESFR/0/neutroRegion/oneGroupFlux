FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      oneGroupFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform 0;
    }

    wedgeY+
    {
        type            wedge;
    }
    
    wedgeY-
    {
        type            wedge;
    }
}

