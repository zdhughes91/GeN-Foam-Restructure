thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       33.4266;
    }
    equationOfState
    {
       rho0             4.125e+03;
       T0               900;
       beta             0;
    }
    thermodynamics
    {
        Cp              1600;
        Hf              0;
        Sf              0;
    }
    transport
