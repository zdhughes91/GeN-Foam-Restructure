FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;//uniform 1e5;

boundaryField
{
    bottom
    {
        type            cyclic;
    }
   
    top
    {
        type            cyclic;
    }

    walls
    {
        type            fixedFluxPressure;
        value           $internalField;

    }

}

