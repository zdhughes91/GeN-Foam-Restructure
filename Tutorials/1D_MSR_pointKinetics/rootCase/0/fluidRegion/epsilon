FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    bottom
    {
        type            cyclic;
    }
   
    top
    {
        type            cyclic;
    }

    walls
    {
       type            epsilonWallFunction;
        value           uniform 0.1;
    }

}

