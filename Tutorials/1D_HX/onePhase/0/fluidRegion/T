FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 653.15;

boundaryField
{
    inletP
    {
        type            fixedValue;
        value           $internalField;
    }
    inletS
    {
        type            fixedValue;
        value           $internalField;
    }
    outletP
    {
        type            zeroGradient;
    }
    outletS
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
}

