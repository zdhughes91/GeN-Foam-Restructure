FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{

    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    topwall
    {
        type            calculated;
        value           $internalField;
    }
    bottomwall
    {
        type            calculated;
        value           $internalField;
    }
    reflector
    {
        type            calculated;
        value           $internalField;
    }
    hx
    {
        type            calculated;
        value           $internalField;
    }
}

