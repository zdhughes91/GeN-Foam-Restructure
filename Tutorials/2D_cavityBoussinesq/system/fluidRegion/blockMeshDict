            (12 15 11 8)
            (1 2 6 5)
            (2 9 13 6)
            (9 10 14 13)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (4 5 6 7)
            (6 13 12 7)
            (13 14 15 12)
            (1 0 3 2)
            (2 3 8 9)
            (9 8 11 10)
        );
    }
