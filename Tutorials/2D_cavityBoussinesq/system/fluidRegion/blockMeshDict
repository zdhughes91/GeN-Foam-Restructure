(
    ($x0 $y0 $z0)
    ($x1 $y0 $z0)
    ($x1 $y1 $z0)
    ($x0 $y1 $z0)
    ($x0 $y0 $z1)
    ($x1 $y0 $z1)
    ($x1 $y1 $z1)
    ($x0 $y1 $z1)
    ($x0 $y2 $z0)
    ($x1 $y2 $z0)
    ($x1 $y3 $z0)
    ($x0 $y3 $z0)
    ($x0 $y2 $z1)
    ($x1 $y2 $z1)
    ($x1 $y3 $z1)
    ($x0 $y3 $z1)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) low (40 10 1) simpleGrading (1 1 1)
    hex (3 2 9 8 7 6 13 12) mid (40 20 1) simpleGrading (1 1 1)
    hex (8 9 10 11 12 13 14 15) top (40 10 1) simpleGrading (1 1 1) 
);

edges
(
);

