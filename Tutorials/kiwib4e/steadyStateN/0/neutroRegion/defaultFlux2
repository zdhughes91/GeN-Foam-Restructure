FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      defaultFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -2 -1 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    inletCentralUnloaded
    {
        type    fixedValue;
        value   uniform 0;
    }

    inletFuelElement
    {
        type    fixedValue;
        value   uniform 0;
    }

    outletCentralUnloaded
    {
        type    fixedValue;
        value   uniform 0;
    }

    outletFuelElement
    {
        type    fixedValue;
        value   uniform 0;
    }

    defaultFaces
    {
        type    zeroGradient;
    }
}


